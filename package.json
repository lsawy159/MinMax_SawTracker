{
  "name": "minmax-sawtracker",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "MinMax SawTracker - نظام متقدم لإدارة الموظفين والشركات",
  "author": "MinMax Team",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:prod": "tsc -b && BUILD_MODE=prod vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "validate": "pnpm type-check && pnpm lint && pnpm test",
    "clean": "rimraf node_modules .pnpm-store dist .turbo coverage",
    "clean:cache": "rimraf node_modules/.vite node_modules/.cache",
    "reinstall": "pnpm clean && pnpm install",
    "prepare": "husky install",
    "analyze": "vite-bundle-visualizer"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.4",
